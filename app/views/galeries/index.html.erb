
<h2>GALERIE DE PEINTURES</h2>
  <% @galeries.each do |galerie| %>
    <% next if galerie == galerie.sculptures %>
      <div class="card-galerie">
        <p><%= galerie.description %></p>
        <%= cl_image_tag galerie.photo.key %>
      </div>
  <% end %>

<h2>GALERIE DE SCULPTURES</h2>
  <div class="card-galerie">
    <p><%= galerie.sculptures.description %></p>
    <%= cl_image_tag galerie.sculptures.photo.key %>
  </div>
